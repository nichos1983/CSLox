class Cake
{
    init()
    {
        this.flavor = "default flavor";
        this.name = "Jack";
    }

    taste()
    {
        print this.name + " love " + this.flavor + " cake.";
    }

    setProperties(name, flavor)
    {
        this.name = name;
        this.flavor = flavor;
    }
}

var cake = Cake();
cake.flavor = "sweety";
cake.taste();
cake.setProperties("I", "spicy");
cake.taste();