class Cake
{
    taste()
    {
        print "The " + this.flavor + " cake is delicious.";
    }
}

var cake = Cake();
cake.flavor = "sweety";
cake.taste();

class Thing
{
    getCallback()
    {
        fun localFunction()
        {
            print this;
        }

        return localFunction;
    }
}

var callback = Thing().getCallback();
callback();